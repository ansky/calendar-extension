{
  "manifest_version": 3,
  "name": "Panda Assistant",
  "version": "1.0",
  "description": "A panda creates calendar events.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/panda_icon_16x16.png",
      "32": "images/panda_icon_32x32.png",
      "48": "images/panda_icon_48x48.png",
      "128": "images/panda_icon_128x128.png"
    }
  },
  "icons": {
    "16": "images/panda_icon_16x16.png",
    "32": "images/panda_icon_32x32.png",
    "48": "images/panda_icon_48x48.png",
    "128": "images/panda_icon_128x128.png"
},
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "identity"
  ],
  "host_permissions": [
    "https://www.googleapis.com/",
    "https://generativelanguage.googleapis.com/"

  ],
  "oauth2": {
    "client_id" : "833320118734-eufl1u5bmtq1v2sj51jk1kuddl7rmujs.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/calendar",
        "https://www.googleapis.com/auth/userinfo.email"]
  }
}
